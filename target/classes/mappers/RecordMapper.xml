<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper

    PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"

    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">




<mapper namespace="com.freehoon.web.board.RecordMapper">



	<select id="getRecord" parameterType="Object"  resultType="com.freehoon.web.record.model.RecordVO">

		SELECT

		content, reg_dt

		FROM

		personal_record

		WHERE
		

		identifier = #{DBDATA.identifier}

	</select>

</mapper>